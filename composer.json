{
  "name": "ccu-plus/eloquent-orm",
  "description": "Eloquent ORM for CCU PLUS API Service.",
  "keywords": ["ccu-plus"],
  "version": "1.0.0",
  "license": "AGPL-3.0-only",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "bepsvpt@protonmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/auth": "^6.2",
    "illuminate/contracts": "^6.2",
    "illuminate/database": "^6.2",
    "illuminate/support": "^6.2",
    "laravel/scout": "^7.2",
    "nesbot/carbon": "^2.25"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "CCUPLUS\\EloquentORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CCUPLUS\\EloquentORM\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/migrations"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
